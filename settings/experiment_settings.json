{
    "experiments": [
        {
        "basic_settings": {
          "target_labelled": "telegram_gold",
          "target_unlabelled": "telegram_unlabeled",
          "sources": [
            "germeval2018", 
            "germeval2019"
        ],
        "labelled_size": 0.05,
        "unlabelled_size": 20000,
        "stratify_unlabelled": true,
        "equal_labelled_size": false,
        "epochs": 2,
        "batch_size": 8,
        "weight_decay": 0,
        "lr": 2e-5,
        "momentum": 0,
        "lr_scheduler": true,
        "validation_split": 0.1,
        "train_split": 0.05,
        "num_workers": 2,
        "seed": 42,
        "test_after_each_epoch": true,
        "freeze_BERT_weights": true,
        "save_model": true,
        "verbose": true
        },
        "exp_settings": [
          
  
            {
              "exp_type": "M3SDA",
              "exp_name": "M3SDA_cls",
              "feature_extractor": "bert_cls",
              "task_classifier": "DANN_task_classifier",
              "domain_classifier": "DANN_domain_classifier",
              "alignment_component": "M3SDA",
              "model": "M3SDA",
              "lamda": 10,
              "eta": 1.0,
              "gamma": 10,
              "mu": 1e-2
            }
          
          
          
        ]
      }
    ]
}